<?xml version="1.0"?>
<tweak name="core/shaders/default_render_templates" extension="render_template_database">
    <search>
        <?xml version="1.0" encoding="utf-8" ?>
        <diesel_render_system/>
    </search>
    <target>
    <render_templates>
	<template name="Gouraud" bind_shader_parameters="true">
			<technique name="default">
				<method name="Gouraud"/>
			</technique>
		</template>		
		<template name="Diffuse" bind_shader_parameters="true">
			<technique name="default">
				<method name="Diffuse"/>
			</technique>
		</template>		
		<template name="Sprite" bind_shader_parameters="true">
			<technique name="default">
				<method name="Sprite"/>
			</technique>
		</template>		
		<template name="Halo" bind_shader_parameters="true">
			<technique name="default">
				<method name="Halo"/>
			</technique>
		</template>		
    </render_templates>
    </target>
</tweak>
