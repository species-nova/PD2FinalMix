				<object name="g_door_left_lod1" enabled="true" max_distance="8000" />
				<object name="g_door_left_lod2" enabled="true" />
			</lod_object>
		</graphic_group>	

		<graphic_group name="grp_door_right" enabled="true" visibility="true" >
