				<object name="g_wheel3_lod1" enabled="true" max_distance="8000" />
				<object name="g_wheel3_lod2" enabled="true" />				
			</lod_object>
		</graphic_group>	

		<object name="s_s_front_left" enabled="true" shadow_caster="true" />
		<object name="s_s_back_left" enabled="true" shadow_caster="true" />
		<object name="s_s_body" enabled="true" shadow_caster="true" />
		<object name="s_s_front_right_2" enabled="true" shadow_caster="true" />
		<object name="s_s_wheel2" enabled="true" shadow_caster="true" />
		<object name="s_s_wheel3" enabled="true" shadow_caster="true" />
	</graphics>

</dynamic_object>
